{
	"name": "icybee/module-images",
	"type": "icanboogie-module",
	"description": "Manages the images uploaded by the users of the CMS Icybee.",
	"keywords": [
		"file",
		"upload"
	],
	"license": "BSD-3-Clause",
	"authors": [
		{
			"name": "Olivier Laviale",
			"email": "olivier.laviale@gmail.com",
			"homepage": "http://olvlvl.com/",
			"role": "Developer"
		}
	],
	"support": {
		"issues": "https://github.com/Icybee/module-images/issues",
		"source": "https://github.com/Icybee/module-images"
	},
	"minimum-stability": "dev",
	"prefer-stable": true,
	"prefer-dist": true,
	"require": {
		"php": ">=5.6",
		"icanboogie/module-installer": "^4.0",
		"icybee/core": "^4.0",
		"icybee/module-files": "^4.0",
		"icybee/module-registry": "^4.0",
		"icybee/module-thumbnailer": "^4.0"
	},
	"autoload": {
		"psr-4": {
			"Icybee\\Modules\\Images\\": "lib"
		}
	},
	"autoload-dev": {
		"classmap": [
			"tests/Application.php"
		]
	},
	"scripts": {
		"post-autoload-dump": "ICanBoogie\\Autoconfig\\Hooks::on_autoload_dump"
	}
}
