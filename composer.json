{
	"name": "icybee/module-images",
	"type": "icanboogie-module",
	"description": "Manages the images uploaded by the users of the CMS Icybee.",
	"keywords": [ "file", "upload" ],
	"license": "BSD-3-Clause",

	"authors": [
		{
			"name": "Olivier Laviale",
			"email": "olivier.laviale@gmail.com",
			"homepage": "http://www.weirdog.com/",
			"role" : "Developer"
		}
	],

	"support": {
		"issues": "https://github.com/Icybee/module-images/issues",
		"source": "https://github.com/Icybee/module-images"
	},

	"require": {
		"php": ">=5.4.0",
		"icanboogie/module-installer": "~1.2",
		"icybee/module-files": "2.1.x@dev",
		"icybee/module-registry": "~2.0",
		"icybee/module-thumbnailer": "~2.1.0"
	},

	"require-dev": {
		"icanboogie/icanboogie": "~2.0",
		"icanboogie/operation": "~2.0",
		"icanboogie/i18n": "~2.0",
		"icybee/core": "~2.0"
	},

	"autoload": {
		"classmap": [ "lib" ]
	},

	"scripts": {
		"post-autoload-dump": "ICanBoogie\\AutoConfig\\Hooks::on_autoload_dump"
	}
}
